# increases key speed 
#xset r rate 300 50 
# uses whatever environmental variables are set in bash_env
if [ -f ~/.zshenv  ]; then
        ~/.zshenv
fi

# sets the caps lock key to also be an escape key, settings for keychron
setxkbmap -rules "evdev" -model "pc105+inet" -layout "us" -option "caps:swapescape"
$HOME/.bin/startup.sh
$HOME/.bin/autoscroll_enabled.sh
export QT_STYLE_OVERRIDE=kvantum
